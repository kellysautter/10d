               ActivateMetaOI_ByName( vSubtask, vDialogTemp2, 0, zREFER_DIALOG_META, zSINGLE, szDialogTag, 0 )
               SET CURSOR FIRST vDialogTemp2.Window WHERE vDialogTemp2.Window.Tag = szWindowTag
               IF RESULT >= zCURSOR_SET
