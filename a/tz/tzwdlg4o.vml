   szWriteBuffer = "   document.body.style.cursor = ^default^;"
   WL_QC( vDialog, lFileJSP, szWriteBuffer, "^", 1 )

   szWriteBuffer = "}"
   WL_QC( vDialog, lFileJSP, szWriteBuffer, "^", 1 )

   // ******************************   End of _AfterPageLoaded ***********************

   //KJS 10/19/07 - This is a generic function for any grids that have a
