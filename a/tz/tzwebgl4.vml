                        STRING ( 100 )   szIndent,
                        INTEGER          lTableRowCnt,
                        STRING ( 1 )     szNoPositioning,
                        STRING ( 100 )   szRepeatGrpKey )

   INTEGER        X_Size
   INTEGER        Y_Size
   INTEGER        X_Pos
