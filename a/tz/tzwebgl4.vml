         zSearchAndReplace( szText2, 1000, QUOTES, szSrch )
         szSrch = ""
         IF szTextID = ""
            szText = "<%=LangConv.getLanguageText(^" + szText2 + "^)%>" 
         ELSE 
            szText = "<%=LangConv.getLanguageText(^" + szText2 + "^, ^" + szTextID + "^)%>" 
         END
         //szText = "<%=LangConv.getLanguageText(^" + szText2 + "^)%>" 
      ELSE
