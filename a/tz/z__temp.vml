      //END
      //WL_QC( vDialogRoot, lFile, szWriteBuffer, "^", 0 )
      
            szWriteBuffer = "%>"
            WL_QC( vDialog, lFile, szWriteBuffer, "^", 0 )

            IF vDialog.Control.CSS_Class = ""
               szClass = ""
            ELSE
               szClass = " class=^" + vDialog.Control.CSS_Class + "^"
            END
         
            szHTML5Attr = ""
