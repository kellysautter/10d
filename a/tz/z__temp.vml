      WL_QC( vDialog, lFile, szWriteBuffer, "^", 1 )

      szWriteBuffer = "<%"
      WL_QC( vDialog, lFile, szWriteBuffer, "^", 0 )
      IF vDialog.CtrlMapView EXISTS


         // See if a list entity has been specified, and if so, use it.
