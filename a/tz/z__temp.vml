            szListEntityName = vDialog.CtrlMapLOD_Entity.Name
         END

         szWriteBuffer = "   " + vDialog.CtrlMapView.Name + " = " +
                         "task.getViewByName( ^" + vDialog.CtrlMapView.Name + "^ );"
